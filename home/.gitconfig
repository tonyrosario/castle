[user]
  name = Tony Rosario
  email = tony.rosario@gmail.com
[github]
  user = tonyrosario
[apply]
  whitespace = nowarn
[alias]
  co = checkout
  cob = checkout -b
  st = status
  br = branch
  ci = commit -a -m
  pom = push origin master
  phm = push heroku master
  pl = pull --rebase
  di = diff --color-words
  weekreport = shortlog --since=1.weeks --author=tonyrosario
  gl = log --graph --abbrev-commit --pretty=oneline --decorate
  this = !git init && git add . && git commit -m "initial commit"
  amend = !git log -n 1 --pretty=tformat:%s%n%n%b | git commit -F - --amend
[color]
  ui = true
[core]
  excludesfile = ~/.gitignore_global
  editor = subl -n -w
[push]
  default = upstream
[difftool]
  prompt = false
[mergetool]
  prompt = false
